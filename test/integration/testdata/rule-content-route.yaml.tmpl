type: route-rule
name: content-route
namespace: {{.Namespace}}
spec:
  destination:
    name: {{.Destination}}
  precedence: 2
  match:
    source:
      name: {{.Source}}
      labels:
        version: v1
    request:
      headers:
        version:
          exact: v2
  route:
    - labels:
         version: v2
